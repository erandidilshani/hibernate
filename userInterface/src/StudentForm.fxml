<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="486.0" prefWidth="328.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="StudentFormController">
   <children>
      <Label alignment="CENTER" layoutX="14.0" layoutY="14.0" prefHeight="32.0" prefWidth="303.0" text="Stuent Form">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <TextField fx:id="txtid" layoutX="23.0" layoutY="62.0" onAction="#searchOnAction" prefHeight="28.0" prefWidth="276.0" promptText="Student Id" />
      <TextField fx:id="txtName" layoutX="23.0" layoutY="100.0" prefHeight="28.0" prefWidth="276.0" promptText="Student Name" />
      <TextField fx:id="txtAddress" layoutX="23.0" layoutY="139.0" prefHeight="28.0" prefWidth="276.0" promptText="Student Address" />
      <TextField fx:id="txtContact" layoutX="23.0" layoutY="178.0" prefHeight="28.0" prefWidth="276.0" promptText="Student Contact" />
      <Button layoutX="235.0" layoutY="227.0" mnemonicParsing="false" onAction="#saveOnAction" prefHeight="28.0" prefWidth="64.0" text="Save" />
      <Button layoutX="161.0" layoutY="227.0" mnemonicParsing="false" onAction="#DeleteOnAction" prefHeight="28.0" prefWidth="64.0" text="Delete" />
      <TableView fx:id="tbl" layoutX="10.0" layoutY="272.0" prefHeight="200.0" prefWidth="303.0">
        <columns>
          <TableColumn fx:id="colid" prefWidth="75.0" text="Id" />
          <TableColumn fx:id="colname" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="coladdress" prefWidth="75.0" text="Address" />
            <TableColumn fx:id="colcontact" prefWidth="75.0" text="Contact" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
